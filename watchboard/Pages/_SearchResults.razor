@using WatchBoard.Services.Database.Entities

<div class="divider">Search</div>

<div class="flex flex-wrap gap-4 ml-4">
    @foreach (var item in Items)
    {
        <_Item ItemModel="item"></_Item>
    }
</div>

@code {

    [Parameter]
    public Board SelectedBoard { get; set; } = new();
    
    [Parameter]
    public List SelectedList { get; set; } = new() {Items = []};
    
    [Parameter]
    public List<Item> Items { get; set; } = [];
}